<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.shuhei-h.automations.usb-watcher</string>
    <key>Program</key>
    <string>{{HOME}}/.local/bin/usb_watcher</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/com.shuhei-h.automations.usb-watcher.stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.shuhei-h.automations.usb-watcher.stderr.log</string>
</dict>
</plist>